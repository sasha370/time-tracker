<div class="container">

  <h2 class="text-center">Audit Logs</h2>

  <table class="table table-striped table-hover">
    <thead>
    <tr>
      <th> ID</th>
      <th> Employee</th>
      <th> Week Starting</th>
      <th> Confirmed at</th>
      <th> Status</th>
    </tr>
    </thead>
    <tbody id='audit_logs'>
    <%= render @audit_logs %>
    </tbody>
  </table>
  <div id="paginator">
    <%= paginate @audit_logs %>
  </div>
</div>